{% extends 'base.html.twig' %}

{% block body %}
    <h1>Our offer</h1>
    <table class="table">
        <tr>
            <td>Make</td>
            <td>Name</td>
        </tr>
        {% for car in cars %}
            <tr>
                <td>{{ car.make }}</td>
                <td>{{ car.name }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}